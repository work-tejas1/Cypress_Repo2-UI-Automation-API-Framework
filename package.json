{
  "name": "cypress_automation_repo2",
  "version": "1.0.0",
  "description": "Automation using cypress",
  "main": "index.js",
  "scripts": {
    "cversion": "cypress --version",
    "test": "cypress run ",
    "test:ci": "NO_COLOR=1 cypress run",
    "createReport": "node cucumber-html-reports.js",
    "open": "node_modules/.bin/cypress open",
    "TestCases_All": "npm run test -- --reporter mochawesome --spec cypress/integration/TestCases_All/**",
    "testReport": "npm run test && npm run createReport"
  },
  "keywords": [
    "work_tejas1"
  ],
  "author": "work_tejas1",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.3.4",
    "clipboardy": "^2.3.0",
    "cypress": "^9.5.2",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-dark": "^1.8.0",
    "cypress-each": "^1.10.0",
    "cypress-wait-until": "^1.7.1",
    "cypress-xpath": "^1.6.2",
    "mochawesome": "^6.2.2",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.2.0",
    "multiple-cucumber-html-reporter": "^1.19.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
  "dependencies": {
    "mocha": "^8.4.0"
  }
}
